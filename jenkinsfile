pipeline
{
    agent any
    stages{
        stage("Checkout from git "){
            steps{
               git url: "url"
               stash includes: "**" , name: "first_stash"
            }
        stage("Checkout from git "){
            steps{
                agent {label 'sonar'}
                script{
                    unstash "first_stash"
                    sh 'mvn clean package'
                }
            }
        }
        }

    }
}
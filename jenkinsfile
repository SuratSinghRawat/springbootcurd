pipeline
{
    agent any
    stages{
        stage("Checkout from git "){
            steps{
                script{
                  sh 'git clone sb-backend main https://github.com/SuratSinghRawat/springbootcurd.git '
                }

              // git url: "https://github.com/SuratSinghRawat/springbootcurd.git"
               stash includes: "**" , name: "first_stash"
            }
        }
        stage("Packaging build "){
            agent {label 'sonar'}
            steps{
                script{
                    unstash "first_stash"
                    sh 'mvn clean package'
                }
            }
        }
    }
}

pipeline
{
    agent any
    stages{
        stage("Checkout from git "){
            steps{
               git branch: 'main', url: 'https://github.com/SuratSinghRawat/springbootcurd.git'
               stash includes: "**" , name:"first_stash"
            }
        }
        stage("Packaging build "){
            agent {label 'sonar'}
            steps{
                  unstash "first_stash"
                  sh 'mvn clean package'
            }
        }
    }
}
